<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    tools:context=".itinerary3"
    android:background="@color/biruSlebew">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/bar3"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rekomendasi wisata berdasar kepribadian"
        android:textColor="@color/putihSlebew"
        android:textSize="15sp"
        android:layout_marginTop="10sp"
        android:layout_marginBottom="10sp"
        android:textAlignment="textStart"/>

    <ImageButton

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/kawahijen"
        android:backgroundTint="@color/biruSlebew"
        android:background="@drawable/card_round"/>

    <ImageButton
        android:id="@+id/btn_wisata"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/biruSlebew"
        android:src="@drawable/tlkhijau"/>
</LinearLayout>